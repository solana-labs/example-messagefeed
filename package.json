{
  "name": "messagefeed",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "start": "babel-node src/cli.js",
    "start-server": "babel-node src/server.js",
    "lint": "npm run pretty && eslint .",
    "lint:fix": "npm run lint -- --fix",
    "flow": "flow",
    "flow:watch": "watch 'flow' . --wait=1 --ignoreDirectoryPattern=/doc/",
    "lint:watch": "watch 'npm run lint:fix' . --wait=1",
    "test": "npm run lint && echo TODO enable: npm run flow",
    "build": "webpack --mode=production",
    "builddev": "webpack --mode=development",
    "dev": "webpack-dev-server --config ./webpack.config.js --mode development",
    "build:bpf-c": "V=1 make -C program-bpf-c",
    "clean:bpf-c": "make -C program-bpf-c clean",
    "build:bpf-rust": "./program-bpf-rust/build.sh",
    "clean:bpf-rust": "./program-bpf-rust/clean.sh",
    "localnet:update": "solana-localnet update beta",
    "localnet:up": "set -x; solana-localnet down beta; set -e; solana-localnet up beta",
    "localnet:deploy": "./program-native/deploy.sh",
    "localnet:down": "solana-localnet down beta",
    "localnet:logs": "solana-localnet logs -f",
    "pretty": "prettier --write '{,src/**/}*.js'",
    "postinstall": "npm run build"
  },
