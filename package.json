{
  "name": "messagefeed",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "start": "babel-node src/cli.js",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "flow": "flow",
    "server": "node dist/server.js",
    "dev-server": "npm run builddev && node dist/server.js",
    "flow:watch": "watch 'flow' . --wait=1 --ignoreDirectoryPattern=/doc/",
    "lint:watch": "watch 'npm run lint:fix' . --wait=1",
    "test": "npm run lint && echo TODO enable: npm run flow",
    "bpf-sdk:update": "node_modules/@solana/web3.js/bin/bpf-sdk-install.sh",
    "build": "webpack --mode=production",
    "builddev": "webpack --mode=development",
    "dev": "webpack-dev-server --config ./webpack.config.js --mode development",
    "build:bpf-c": "V=1 make -C bpf-c-programs/messagefeed",
    "clean:bpf-c": "make -C bpf-c-programs/messagefeed clean",
    "build:bpf-rust": "./bpf-rust-programs/messagefeed/do.sh build",
    "clean:bpf-rust": "./bpf-rust-programs/messagefeed/do.sh clean",
    "localnet:update": "solana-localnet update",
    "localnet:up": "set -x; solana-localnet down; set -e; solana-localnet up",
    "localnet:deploy": "./program-native/deploy.sh",
    "localnet:down": "solana-localnet down",
    "localnet:logs": "solana-localnet logs -f",
    "postinstall": "npm run build; npm run bpf-sdk:update"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "copy-webpack-plugin": "^5.0.4",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "prettier": "^1.18.2",
    "webpack-filter-warnings-plugin": "^1.2.1"
  },
  "dependencies": {
    "@material-ui/core": "^3.9.3",
    "@material-ui/icons": "^3.0.2",
    "@solana/web3.js": "^0.18.7",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-function-bind": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "buffer-layout": "^1.2.0",
    "cors": "^2.8.5",
    "css-loader": "^3.2.0",
    "escape-html": "^1.0.3",
    "eslint": "^5.3.0",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-react": "^7.14.3",
    "event-emitter": "^0.3.5",
    "express": "^4.17.1",
    "flow-bin": "0.105.2",
    "flow-typed": "^2.6.1",
    "haikunator": "^2.1.1",
    "http-server": "^0.11.1",
    "localforage": "^1.7.3",
    "mkdirp-promise": "^5.0.1",
    "moment": "^2.22.2",
    "mz": "^2.7.0",
    "node-fetch": "^2.6.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-idle-timer": "^4.2.8",
    "readline-promise": "^1.0.3",
    "superstruct": "^0.6.0",
