{
  "name": "messagefeed",
  "version": "0.0.1",
  "description": "",
  "testnetDefaultChannel": "edge",
  "scripts": {
    "message-cli": "babel-node src/cli/message-feed.js",
    "poll-cli": "npm run build:poll-cli && node dist/cli/prediction-poll.js",
    "lint": "npm run pretty && eslint .",
    "lint:fix": "npm run lint -- --fix",
    "flow": "flow",
    "server": "node dist/server.js",
    "dev-server": "npm run builddev && node dist/server.js",
    "flow:watch": "watch 'flow' . --wait=1 --ignoreDirectoryPattern=/doc/",
    "lint:watch": "watch 'npm run lint:fix' . --wait=1",
    "test": "npm run lint && echo TODO enable: npm run flow",
    "bpf-sdk:update": "solana-bpf-sdk-install node_modules/@solana/web3.js",
    "build": "webpack --mode=production",
    "builddev": "webpack --mode=development",
    "dev": "webpack-dev-server --config ./webpack.config.js --mode development",
    "build:poll-cli": "webpack --mode development --config webpack.cli.js",
    "build:bpf-c": "V=1 make -C bpf-c-programs/messagefeed",
    "clean:bpf-c": "make -C bpf-c-programs/messagefeed clean",
    "build:bpf-rust": "npm run build:bpf-rust-mf && npm run build:bpf-rust-pp",
    "clean:bpf-rust": "npm run clean:bpf-rust-mf && npm run clean:bpf-rust-pp",
    "build:bpf-rust-mf": "./bpf-rust-programs/messagefeed/do.sh build",
    "clean:bpf-rust-mf": "./bpf-rust-programs/messagefeed/do.sh clean",
    "build:bpf-rust-pp": "./bpf-rust-programs/prediction-poll/program/do.sh build",
    "clean:bpf-rust-pp": "./bpf-rust-programs/prediction-poll/program/do.sh clean",
    "localnet:update": "solana-localnet update",
    "localnet:up": "set -x; solana-localnet down; set -e; solana-localnet up",
    "localnet:down": "solana-localnet down",
    "localnet:logs": "solana-localnet logs -f",
    "pretty": "prettier --write '{,src/**/}*.js'",
    "postinstall": "npm run build; npm run bpf-sdk:update"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-flow": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@wasm-tool/node": "^0.1.0",
    "@wasm-tool/wasm-pack-plugin": "^1.3.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "copy-webpack-plugin": "^6.0.2",
    "css-loader": "^3.5.3",
    "eslint": "^6.8.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-react": "^7.20.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "flow-bin": "0.124.0",
    "flow-typed": "^3.1.0",
    "prettier": "^2.0.5",
    "watch": "^1.0.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-filter-warnings-plugin": "^1.2.1"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.14",
    "@material-ui/icons": "^4.9.1",
    "@solana/web3.js": "^0.49.2",
    "buffer-layout": "^1.2.0",
    "cors": "^2.8.5",
    "event-emitter": "^0.3.5",
    "express": "^4.17.1",
    "haikunator": "^2.1.2",
    "http-server": "^0.12.3",
    "localforage": "^1.7.3",
    "mkdirp-promise": "^5.0.1",
    "moment": "^2.25.3",
    "mz": "^2.7.0",
    "node-fetch": "^2.6.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-idle-timer": "^4.2.12",
    "react-router-dom": "^5.2.0",
    "readline-promise": "^1.0.3",
    "semver": "^7.3.2",
    "superstruct": "^0.8.3"
  },
  "engines": {
    "node": ">=11"
  }
}
